{
  "name": "sutro",
  "version": "1.0.0",
  "description": "API Resource Framework",
  "main": "dist/index.js",
  "keywords": [
    "api",
    "realtime",
    "rest",
    "http",
    "express",
    "middleware",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shastajs/sutro.git"
  },
  "author": "Contra <yo@contra.io> (http://contra.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shastajs/sutro/issues"
  },
  "homepage": "https://github.com/shastajs/sutro#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build && npm docs",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-register --recursive --reporter spec && npm run-script lint",
    "docs": "npm run docs:pre && npm run docs:build && npm run docs:publish",
    "docs:pre": "gitbook install && rimraf _book",
    "docs:build": "gitbook build -g shastajs/sutro",
    "docs:publish": "cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:shastajs/sutro gh-pages --force"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "eslint": "^3.11.1",
    "eslint-config-rackt": "^1.1.1",
    "gitbook-cli": "^2.3.0",
    "github-changes": "^1.0.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "should": "^11.1.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "rackt",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "it": true,
      "describe": true
    }
  },
  "dependencies": {
    "dot-prop": "^4.1.1",
    "express": "^4.0.0",
    "handle-async": "^1.0.1",
    "pluralize": "^4.0.0",
    "url-join": "^2.0.1"
  }
}
